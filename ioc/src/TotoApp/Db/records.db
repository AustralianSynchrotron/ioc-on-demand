record(calc, "$(PREFIX):TEMPERATURE") {
  field(DESC, "A noisy temperature reading")
  field(CALC, "21.5 + RNDM")
  field(SCAN, "1 second")
  field(PINI, "YES")
  field(EGU, "C")
  field(PREC, 3)
  field(HOPR, 1)
  field(LOPR, 0)
  field(HIHI, 22.25)
  field(HIGH, 22.45)
  field(LOW, 21)
  field(LOLO, 20)
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
  field(LSV, "MINOR")
  field(LLSV, "MAJOR")
}

record(calc, "$(PREFIX):FAST_RANDOM") {
  field(CALC, "RNDM")
  field(SCAN, ".1 second")
  field(PINI, "YES")
  field(EGU, "mm")
  field(PREC, 3)
  field(HOPR, 1)
  field(LOPR, 0)
  field(HIHI, .9)
  field(HIGH, .75)
  field(LOW, .25)
  field(LOLO, .1)
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
  field(LSV, "MINOR")
  field(LLSV, "MAJOR")
}

record(ao, "$(PREFIX):X") {
  field(DESC, "Store a number between 0 and 100")
  field(VAL, 2)
  field(PINI, "YES")
  field(HOPR, 100)
  field(LOPR, 0)
  field(DRVH, 100)
  field(DRVL, 0)
  field(HIHI, 95)
  field(HIGH, 90)
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
}

record(ao, "$(PREFIX):Y") {
  field(DESC, "Store a number between 0 and 100")
  field(VAL, 3)
  field(PINI, "YES")
  field(HOPR, 100)
  field(LOPR, 0)
  field(DRVH, 100)
  field(DRVL, 0)
  field(HIHI, 95)
  field(HIGH, 90)
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
}

record(calc, "$(PREFIX):X_TIMES_Y") {
  field(DESC, "X times Y")
  field(SCAN, "Passive")
  field(PINI, "YES")
  field(CALC, "A * B")
  field(INPA, "$(PREFIX):X CP")
  field(INPB, "$(PREFIX):Y CP")
  field(HOPR, 10000)
  field(LOPR, 0)
  field(HIHI, 5000)
  field(HIGH, 1000)
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
}

record(stringout, "$(PREFIX):SHORT_STRING") {
  field(DESC, "A short string")
  field(VAL, "a message up to 40 characters")
  field(PINI, "YES")
}

record(waveform, "$(PREFIX):LONG_STRING") {
  field(DESC, "Bytes which may be text")
  field(PINI, "YES")
  field(FTVL, "UCHAR")
  field(NELM, "500")
}

record(mbbo, "$(PREFIX):ALERT") {
  field(DESC, "Alert level")
  field(VAL, "3")
  field(PINI, "YES")
  field(ZRST, "all clear")
  field(ONST, "nothing to worry about")
  field(TWST, "uh-oh")
  field(THST, "run for the hills")
  field(ZRSV, "NO_ALARM")
  field(ONSV, "NO_ALARM")
  field(TWSV, "MINOR")
  field(THSV, "MAJOR")
}

record(aSub, "$(PREFIX):WAVE_GENERATOR") {
  field(SCAN, "1 second")
  field(SNAM, "processWave")
  field(FTVA, "FLOAT")
  field(NOVA, "1000")
  field(OUTA, "$(PREFIX):WAVE PP")
}

record(waveform, "$(PREFIX):WAVE") {
  field(FTVL, "FLOAT")
  field(NELM, "1000")
}

record(ao, "$(PREFIX):SETPOINT") {
  field(VAL, "7")
  field(PINI, "YES")
  field(DRVL, 0)
  field(DRVH, 10)
  field(LOPR, 0)
  field(HOPR, 10)
  field(EGU, "mm")
}

record(sub, "$(PREFIX):READBACK") {
  field(SCAN, ".1 second")
  field(SNAM, "processReadback")
  field(INPA, "$(PREFIX):SETPOINT")
  field(PREC, 3)
  field(EGU, "mm")
}

record(sub, "$(PREFIX):TEMPERATURE") {
  field(SCAN, "1 second")
  field(INAM, "initSubroutine")
  field(SNAM, "processSubroutine")
  field(PREC, 3)
  field(EGU, "C")
  field(LOLO, 0)
  field(HIHI, 100)
  field(LLSV, MAJOR)
  field(HHSV, MAJOR)
}
